{
  "name": "mijn-app-backend",
  "description": "The app client for the *Mijn App - Mijn Portaal* project",
  "licence": "UNLICENSED",
  "private": true,
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack --env.NODE_ENV=production --optimize-minimize",
    "dev": "webpack-dev-server --hot --inline",
    "pretest": "npm run build",
    "test": "npm run lint && npm run test:lighthouse",
    "selenium": "wct --npm",
    "test:lighthouse": "concurrently --kill-others \"http-server dist\" \"lighthouse --view http://localhost:8080\" ",
    "test:travis": "npm run pretest && npm run lint",
    "clean:lighthouse": "rm *.report.html",
    "lint": "eslint src/**/*.js",
    "lint_old": "eslint src/**/*.js && stylelint src/components/**/*.pcss",
    "release": "standard-version",
    "start": "npm run build && http-server dist"
  },
  "engines": {
    "node": ">= 8.0 < 10"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "eol-last": 0,
      "import/no-unassigned-import": 0,
      "promise/prefer-await-to-then": 0
    }
  },
  "devDependencies": {
    "autoprefixer": "9.0.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "3.6.1",
    "copy-webpack-plugin": "4.5.2",
    "cssnano": "4.0.3",
    "ejs-loader": "0.3.1",
    "eslint": "5.2.0",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-es6": "1.0.0",
    "eslint-plugin-html": "4.0.5",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-node": "7.0.1",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "lighthouse": "3.0.3",
    "postcss": "7.0.1",
    "postcss-cssnext": "3.1.0",
    "postcss-custom-media": "6.0.0",
    "postcss-discard-comments": "4.0.0",
    "postcss-easy-import": "3.0.0",
    "postcss-loader": "2.1.6",
    "postcss-mixins": "6.2.0",
    "postcss-nesting": "6.0.0",
    "postcss-reporter": "5.0.0",
    "postcss-selector-not": "3.0.1",
    "script-ext-html-webpack-plugin": "2.0.1",
    "standard-version": "4.4.0",
    "stylelint": "9.3.0",
    "stylelint-config-standard": "18.2.0",
    "text-loader": "0.0.1",
    "wct-browser-legacy": "1.0.1",
    "web-component-tester": "6.7.1",
    "webpack": "4.16.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "workbox-webpack-plugin": "3.4.1",
    "xo": "0.21.1"
  },
  "dependencies": {
    "@polymer/iron-pages": "3.0.0-pre.21",
    "@polymer/polymer": "3.0.2",
    "@webcomponents/webcomponentsjs": "2.0.3",
    "axios": "0.18.0",
    "prettier": "1.14.2",
    "pwa-helpers": "0.8.3",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2"
  }
}
