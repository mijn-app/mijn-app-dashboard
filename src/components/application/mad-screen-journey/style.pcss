  --icon-color: var(--palette-primary-main);
}
.InfoEditorCloseButton:hover {
  opacity: 0.4;
}
.ModulesSelector {
  width: 789px;
  max-width: calc(100vw - 50px);
  transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1);
}
.ModulesSelector.ModuleSelected {
  transform: translate(-100vw, 0);
}
.SelectedModuleInformation {
  width: 789px;
  max-width: calc(100vw - 50px);
  transform: translate(100vw, 0);
  transition: transform 0.4s cubic-bezier(0.4, 0, 0.2, 1);
}
.SelectedModuleInformation.ModuleSelected {
  transform: translate(0, 0);
}
.ModulesSelectorInner {
  height: 641px;
  max-height: calc(100vh - 109px);
  width: calc(100% - 69px);
  padding: 30px 31px 29px 38px;
  overflow-y: scroll;
  overflow-x: hidden;
}
.ModulesSelectorInnerHeading {
  font-family: var(--typography-fontFamily);
  font-size: 1.78em;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.2;
  letter-spacing: -0.4px;
  color: var(--palette-primary-main);
  margin: 2px 0 0 0;
}
.ModulesSelectorInnerSubHeading {
  font-family: var(--typography-fontFamily);
  font-size: 1.14em;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
