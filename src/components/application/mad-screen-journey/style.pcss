.BottomLeftButtons {
  position: fixed;
  bottom: 26px;
  left: 274px;
}
.PopUpWrapper {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  display: grid;
  grid-template-columns: auto;
  grid-template-rows: auto;
}
.PopUpBackdrop {
  background-color: rgba(0, 0, 0, 0.5);
  grid-column: 1 / 2;
  grid-row: 1 / 2;
}
.PopUp {
  grid-column: 1 / 2;
  grid-row: 1 / 2;
  justify-self: center;
  align-self: center;
  position: relative;
}
.DeleteDialog,
.EditOptionsDialog,
.InfoEditor {
  width: 423px;
  max-width: calc(100vw - 50px);
}
.DeleteDialogContent,
.EditOptionsDialogContent {
  padding: 32px 35px 35px 35px;
}
.DeleteDialogHeader,
.EditOptionsDialogHeader {
  padding-bottom: 3px;
  font-family: var(--typography-fontFamily);
  font-size: 1.79em;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.2;
  letter-spacing: -0.4px;
  color: var(--palette-primary-main);
}
.DeleteDialogBody {
  padding: 5px 0 9px 0;
  font-family: var(--typography-fontFamily);
  font-size: 1.14em;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.2;
  letter-spacing: -0.1px;
  color: var(--palette-primary-main);
}
.DeleteDialogLabel,
.EditOptionsDialogLabel {
  padding: 15px 0 9px 0;
  font-family: var(--typography-fontFamily);
  font-size: 1.14em;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.2;
  letter-spacing: -0.1px;
  color: var(--palette-primary-main);
}
.DeleteDialogButtons,
.EditOptionsDialogButtons {
  width: 100%;
  display: flex;
  margin-top: 15px;
  justify-content: flex-end;
}
.InfoEditorContent {
  padding: 32px 35px 46px 35px;
}
.InfoEditorHeader {
  padding-bottom: 3px;
  font-family: var(--typography-fontFamily);
  font-size: 1.79em;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.2;
  letter-spacing: -0.4px;
  color: var(--palette-primary-main);
}
.InfoEditorLabel {
  padding: 35px 0 9px 0;
  font-family: var(--typography-fontFamily);
  font-size: 1.14em;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.2;
  letter-spacing: -0.1px;
  color: var(--palette-primary-main);
}
.InfoEditorCloseButton,
.EditOptionsDialogCloseButton {
  position: absolute;
  top: 20px;
  right: 20px;
  opacity: 0.2;
